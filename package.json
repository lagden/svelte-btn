{
  "name": "@tadashi/svelte-btn",
  "version": "6.1.1",
  "repository": "github:lagden/svelte-btn",
  "description": "Svelte component",
  "keywords": [
    "svelte",
    "button",
    "component"
  ],
  "svelte": "src/btn.js",
  "main": "src/btn.js",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
